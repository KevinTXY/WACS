{
  "manifest_version": 2,
  "name": "WACS - Web Accessibility and Convenience Suite",
  "version": "1.0.0",
  "options_page": "settings.html",
  "icons": { "16": "resources/accessibility-hexagons.png"},
  "permissions": [
    "storage"
  ],
  "content_scripts": [
   {
     "matches": [
       "<all_urls>"
     ],
     "js": ["libs/jquery-3.2.1.min.js", "libs/textblur.js", "libs/image-blur.js", "libs/anti-gif.js"]
   }
 ]
}
